<import src='movies-template1/movies-template1.wxml' />
<import src='movies-list-template/movies-list-template.wxml' />



<view class='container'>


  <block wx:for="{{local_movies}}">
    <template is='movies-template1' data="{{...item}}" />
    <view class='movie-list'>
      <block wx:for="{{item.movies}}">
        <template is='movies-list-template' data="{{...item}}" />
      </block>
    </view>
  </block>



</view>